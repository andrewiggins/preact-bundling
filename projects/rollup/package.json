{
  "name": "rollup",
  "type": "module",
  "private": true,
  "version": "0.0.1",
  "description": "A Preact app built with Rollup",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"sirv dist --dev\" \"rollup -c -w\"",
    "build": "rollup -c",
    "serve": "sirv dist"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-react": "^7.22.15",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-html": "^1.0.2",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "concurrently": "^8.2.1",
    "rollup": "^3.29.4",
    "sirv-cli": "^2.0.2"
  },
  "dependencies": {
    "preact": "^10.18.1"
  }
}
